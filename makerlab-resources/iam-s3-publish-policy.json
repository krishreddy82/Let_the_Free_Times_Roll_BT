{
  "Statement": [
      {
          "Effect": "Allow",
          "Action": "s3:ListAllMyBuckets",
          "Resource": "arn:aws:s3:::*"
      },
      {
          "Effect": "Allow",
          "Action": "s3:*",
          "Resource": [
            "arn:aws:s3:::east-makertest-bucket-7867867678",
            "arn:aws:s3:::east-makertest-bucket-7867867678/*",
            "arn:aws:s3:::east-makertest-bucket-7867867678/assets/*"
          ]
      }
  ]
}